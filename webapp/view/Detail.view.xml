<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="SKE.UI5Skeleton.controller.Detail"
	xmlns:html="http://www.w3.org/1999/xhtml">
	<App id="idAppControl">
		<pages>
			<Page title="{i18n>DetailTitle}"
			      showNavButton="true"
			      navButtonPress="handleNavButtonPress" >
				<content>
					<VBox>
						<Text text="{purchaseOrder}" />
						<Text text="{purchaseOrganisation}" />
						<Text text="{purchaseDate}" />
						<Text text="{validityStartDate}" />
					</VBox>					
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>